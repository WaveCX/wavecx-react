{
  "name": "@wavecx/wavecx-react",
  "version": "1.7.0",
  "description": "WaveCX React library",
  "homepage": "https://github.com/WaveCX/wavecx-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WaveCX/wavecx-react.git"
  },
  "bugs": {
    "url": "https://github.com/WaveCX/wavecx-react/issues"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "source": "src/index",
  "main": "dist/index.js",
  "style": "dist/styles.css",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "styles.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "workspaces": [
    "example"
  ],
  "scripts": {
    "build": "rimraf dist && tsc && rollup -c",
    "typecheck": "tsc",
    "test": "vitest",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write ./src",
    "deploy-example": "gh-pages -d example/dist",
    "example": "npm --workspace=example run dev"
  },
  "browserslist": [
    "defaults"
  ],
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "gh-pages": "^6.1.1",
    "jsdom": "^25.0.0",
    "msw": "^2.4.5",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^6.0.1",
    "rollup": "^3.20.7",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vitest": "^2.0.5"
  }
}
